%section#home-section-imovel
  .row
    #menu-default.col-lg-12.col-md-12.col-sm-12.col-xs-12.lista-tipo-imovel
      - Property.detail_navs.each do |key, value|
        .col-lg-4.col-md-4.col-sm-4.col-xs-4
          .tipo-imovel
            .menu-text
              %a{:href => "lista/3/1/A"}
                %img.img-responsive{:src => "/images/#{key}.png"}/
  .col-lg-12.col-md-12.col-sm-12.col-xs-12
    #imovel-menu-default
      .col-lg-3.col-md-3.col-sm-3.col-xs-4
        .inner
          .imovel-menu-text
            %a{:href => "javascript: abrirListaAnunciante('', 'A')"}
              %img.img-responsive{:src => "http://meunw.com.br/assets/images/icon-anuncie.png"}/
